{
  "name": "codea/smart-reply",
  "description": "Smart responder supports middlewares based on Symfony HTTP Foundation",
  "type": "library",
  "license": "MIT",
  "authors": [
    {
      "name": "Edo Barnas",
      "email": "iam@edobarnas.com"
    }
  ],
  "config": {
    "platform": {
      "php": "8.1.0",
      "ext-json": "1",
      "ext-intl": "1"
    },
    "sort-packages": true
  },
  "autoload": {
    "psr-4": {
      "Codea\\SmartReply\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Codea\\SmartReply\\Test\\": "tests"
    }
  },
  "require": {
    "php": "^8.1",
    "ext-json": "*",
    "beberlei/assert": "3.3.*",
    "webmozart/assert": "1.10.*",
    "psr/log": "1.*|2.*|3.*",
    "symfony/http-foundation": "5.*|6.*",
    "symfony/routing": "5.*|6.*",
    "symfony/translation-contracts": "3.0.*",
    "twig/twig": "2.12.*|3.*"
  },
  "require-dev": {
    "phpstan/phpstan": "1.6.*",
    "phpstan/phpstan-phpunit": "1.1.*",
    "phpunit/phpunit": "9.5.*",
    "symplify/easy-coding-standard": "10.2.*"
  }
}
